{
  "name": "venko",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy-be": "make packageAndDeployServerless",
    "deploy-ui": "aws s3 cp ./ui s3://venko --recursive --exclude images/*",
    "deploy-ng-ui": "cd ng-ui && ng build --prod && aws s3 cp ./dist/ng-ui s3://venko --recursive",
    "refresh-ui": "aws cloudfront create-invalidation --distribution-id E22YZ4R6C72E8K --paths \"/*\"",
    "build-dependency": "cp package.json ./dist && cd dist && npm install --production"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^6.10.14",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/swagger": "^4.5.1",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.656.0",
    "aws-serverless-express": "^3.3.8",
    "cache-manager": "^3.2.1",
    "dotenv": "^8.2.0",
    "https-proxy-agent": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^1.8.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/aws-lambda": "^8.10.51",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.1",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
